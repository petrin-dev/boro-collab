        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#fff"
                },
                {
                    "name": "Tundora",
                    "slug": "tundora",
                    "color": "#42393C"
                },
                {
                    "name": "White Lilac",
                    "slug": "white-lilac",
                    "color": "#E3E7F4"
                },
                {
                    "name": "Soft Peach",
                    "slug": "soft-peach",
                    "color": "#F7F2F1"
                },
                {
                    "name": "Pearl Bush",
                    "slug": "pearl-bush",
                    "color": "#EFE6E3"
                },
                {
                    "name": "Ecru White",
                    "slug": "ecru-white",
                    "color": "#F1EEDB"
                },
                {
                    "name": "Swirl",
                    "slug": "swirl",
                    "color": "#D6CFC4"
                },
                {
                    "name": "Pastel Gray",
                    "slug": "pastel-gray",
                    "color": "#d6cfc3"
                },
                {
                    "name": "Fuscous Gray",
                    "slug": "fuscous-gray",
                    "color": "#53584C"
                },
                {
                    "name": "Camouflage",
                    "slug": "camouflage",
                    "color": "#7C8276"
                },
                {
                    "name": "Matterhorn",
                    "slug": "matterhorn",
                    "color": "#4F3743"
                },
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#171415"
                }
            ]
        },
		"custom": {
			"color": {
				"base": "var(--wp--preset--color--tundora)",
				"contrast": "var(--wp--preset--color--white-lilac)",
				"accent": "var(--wp--preset--color--camouflage)"
			},
			"default-font": "var(--wp--preset--font-family--mukta)"
		},
        "typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Mukta\", sans-serif",
					"name": "Mukta",
					"slug": "Mukta",
					"fontFace": [
						{
							"fontFamily": "Mukta",
							"fontWeight": "200",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-ExtraLight.ttf" ]
						},
						{
							"fontFamily": "Mukta",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-Light.ttf" ]
						},
						{
							"fontFamily": "Mukta",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-Regular.ttf" ]
						},
						{
							"fontFamily": "Mukta",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-Medium.ttf" ]
						},
						{
							"fontFamily": "Mukta",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-SemiBold.ttf" ]
						},
						{
							"fontFamily": "Mukta",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-Bold.ttf" ]
						},
						{
							"fontFamily": "Mukta",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/Mukta/Mukta-ExtraBold.ttf" ]
						}
					]
				}
			],
			"fontSizes":[
				{
					"slug": "tiny",
					"size": "0.5625rem",
					"name": "Tiny (9px)"
				},
				{
					"slug": "smaller",
					"size": "0.875rem",
					"name": "Smaller (14px)"
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base Size (16px)"
				},
				{
					"slug": "bigger",
					"size": "1.25rem",
					"name": "Bigger (20px)"
				},
				{
					"slug": "heading-3",
					"size": "1.3125rem",
					"name": "H3 (21px)"
				},
				{
					"slug": "heading-2",
					"size": "2.25rem",
					"name": "H2 (36px)"
				},
				{
					"slug": "heading-1",
					"size": "3.625rem",
					"name": "H1 (58px)"
				}
				
			]
		}
    }
}