		"custom": {
			"color": {
				"base": "var(--wp--preset--color--dark-text)",
				"contrast": "var(--wp--preset--color--white)",
				"accent": "var(--wp--preset--color--mac-clay)"
			},
			"default-font": "var(--wp--preset--font-family--dm-sans)"
		},
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "\"Montserrat\", sans-serif",
                    "slug": "montserrat-sans-serif",
                    "name": "Montserrat"
                },
                {
