{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
            "customDuotone": false,
			"customGradient": false,
            "defaultGradients": false,
			"defaultPalette": false,
            "duotone": [],
            "gradients": [],
			"palette": [
				{
					"color": "#292929",
					"name": "Jet",
					"slug": "jet"
				},
				{
					"color": "#E8E1E1",
					"name": "Platinum",
					"slug": "platinum"
				},
				{
					"color": "#7E7F9A",
					"name": "Cool Gray",
					"slug": "cool-gray"
				},
				{
					"color": "#BFAE48",
					"name": "Old Gold",
					"slug": "old-gold"
				},
				{
					"color": "#167E62",
					"name": "Viridian",
					"slug": "viridian"
				}
			]
		},
		"custom": {
			"color": {
				"base": "var(--wp--preset--color--platinum)",
				"contrast": "var(--wp--preset--color--jet)",
				"accent": "var(--wp--preset--color--cool-gray)"
			},
			"default-font": "var(--wp--preset--font-family--dm-sans)",
			"tablet-stack": "1200px",
			"mobile-stack": "600px"
		},
		"layout": {
			"contentSize": "min(1600px, 90vw)"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./fonts/dm-sans/DMSans-Regular.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./fonts/dm-sans/DMSans-Regular-Italic.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./fonts/dm-sans/DMSans-Bold.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./fonts/dm-sans/DMSans-Bold-Italic.woff2"
							]
						}
					],
					"fontFamily": "\"DM Sans\", sans-serif",
					"name": "DM Sans",
					"slug": "dm-sans"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "IBM Plex Mono",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./fonts/ibm-plex-mono/IBMPlexMono-Light.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "IBM Plex Mono",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./fonts/ibm-plex-mono/IBMPlexMono-Regular.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "IBM Plex Mono",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./fonts/ibm-plex-mono/IBMPlexMono-Italic.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "IBM Plex Mono",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./fonts/ibm-plex-mono/IBMPlexMono-Bold.woff2"
							]
						}
					],
					"fontFamily": "'IBM Plex Mono', monospace",
					"name": "IBM Plex Mono",
					"slug": "ibm-plex-mono"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./fonts/inter/Inter-VariableFont_slnt,wght.ttf"
							]
						}
					],
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Source Serif Pro",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./fonts/source-serif-pro/SourceSerif4Variable-Roman.ttf.woff2"
							]
						},
						{
							"fontFamily": "Source Serif Pro",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "200 900",
							"src": [
								"file:./fonts/source-serif-pro/SourceSerif4Variable-Italic.ttf.woff2"
							]
						}
					],
					"fontFamily": "\"Source Serif Pro\", serif",
					"name": "Source Serif Pro",
					"slug": "source-serif-pro"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "10rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			],
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
        "blocks": {
			"core/paragraph": {
                "spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0",
						"left": "0"
					}
				}
            },
            "core/button": {
                "border": {
                    "radius": "0"
                },
				"color": {
					"background": "var(--wp--custom--color--base)",
					"text": "var(--wp--custom--color--contrast)"
				},
                "typography": {
                    "textTransform": "uppercase"
                }
            }
        },
		"color": {
			"background": "var(--wp--custom--color--contrast)",
			"text": "var(--wp--custom--color--base)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--custom--color--base)",
					"text": "var(--wp--custom--color--contrast)"
				},
				"border": {
					"radius": "0",
					"width": "2px",
					"color": "var(--wp--custom--color--base)"
				},
				":hover": {
					"color": {
						"background": "transparent",
						"text": "var(--wp--custom--color--base)"
					},
					"border": {
						"width": "2px",
						"color": "var(--wp--custom--color--contrast)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--custom--color--contrast)",
						"text": "var(--wp--custom--color--base)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--custom--color--contrast)",
						"text": "var(--wp--custom--color--base)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--custom--color--accent)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--accent)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--custom--color--accent)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"top": "0",
				"right": "0",
				"bottom": "0",
				"left": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--custom--default-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	}
}